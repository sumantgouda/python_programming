   "source": [
    "# Swap the number without using third varible\n",
    "a=int(input(\"enter first number:\"))\n",
    "b=int(input(\"enter second number:\"))\n",
    "print(\"Before swapping\")\n",
    "print(f\"a={a}   b={b}\")\n",
    "a,b=b,a\n",
    "print(\"After sapping\")\n",
    "print(f\"a={a}   b={b}\")\n"
   ]
