   "source": [
    "# Write a program that will take three digits from the user and add the square of each digit.\n",
    "n=input(\"enter number:\")\n",
    "s_digit=0\n",
    "for i in n:\n",
    "    s_digit=s_digit+int(i)**2\n",
    "print(f\"sum of squares of {n} digits: {s_digit}\")"
