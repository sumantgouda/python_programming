   "source": [
    "# Write a Program to extract each digit from an integer in the reverse order\n",
    "n=int(input(\"enter number:\"))\n",
    "rev=0\n",
    "while(n>0):\n",
    "    rem=n%10\n",
    "    rev=rev*10+rem\n",
    "    n=n//10\n",
    "print(f\"reverse number:{rev}\")"
   ]
