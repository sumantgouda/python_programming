    "# 13. Print all factors of a given number provided by the user.\n",
    "n=int(input(\"enter number:\"))\n",
    "fact=1\n",
    "for i in range(1,n+1):\n",
    "    fact=fact*i\n",
    "print(f\"factorial of {n} is :{fact}\")"
