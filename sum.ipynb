    "# Write a program that will give you the sum of  digits\n",
    "n=int(input(\"enter number:\"))\n",
    "sum=0\n",
    "while(n>0):\n",
    "    rem=n%10\n",
    "    sum=sum+rem\n",
    "    n=n//10\n",
    "print(f\"sum of digits:{sum}\")\n",
    "        "
